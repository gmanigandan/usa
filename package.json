{
  "name": "docditusa-app",
  "version": "0.1.0",
  "private": true,
  "description": "Online Telemedicine services 24/7 urgent care",
  "author": "DocDIT",
  "main": "./public/electron.js",
  "homepage": ".",
  "dependencies": {
    "@paypal/react-paypal-js": "^7.8.1",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "agora-electron-sdk": "^3.7.0-build.527",
    "antd": "^4.20.7",
    "axios": "^0.27.2",
    "child_process": "^1.0.2",
    "concurrently": "^7.2.1",
    "dns": "^0.2.2",
    "electron-forge": "^5.2.4",
    "electron-is-dev": "^2.0.0",
    "electron-push-receiver": "^2.1.3",
    "electron-store": "^8.0.1",
    "electron-windows-badge": "^1.1.0",
    "moment": "^2.29.4",
    "node-notifier": "^10.0.1",
    "node-wav-player": "^0.2.0",
    "path": "^0.12.7",
    "pushy-electron": "^1.0.9",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-electron-browser-view": "^3.0.0",
    "react-flags-select": "^2.2.3",
    "react-infinite-scroll-component": "^6.1.0",
    "react-otp-input": "^2.4.0",
    "react-phone-input-2": "^2.15.0",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "react-spinners": "^0.12.0",
    "socket.io-client": "^4.5.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "dev": "concurrently \"npm start\" \"wait-on http://localhost:3000 && electron .\"",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "ebuild": "npm run build && node_modules/.bin/build",
    "make": "npm run build && electron-builder -w -c.extraMetadata.main=build/electron.js",
    "make-all": "npm run build && electron-builder -mwl -c.extraMetadata.main=build/electron.js"
  },
  "agora_electron": {
    "electron_version": "7.1.2",
    "prebuilt": true,
    "platform": "win64"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "electron": "^7.1.2",
    "electron-builder": "^23.0.3",
    "wait-on": "^6.0.1"
  },
  "build": {
    "appId": "com.docditusa.app",
    "productName": "DocDIT-USA",
    "copyright": "Copyright Â© 2022 DocDIT",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "extraResources": [
      "./extraResources/**"
    ],
    "directories": {
      "buildResources": "public"
    },
    "mac": {
      "target": "dmg",
      "icon": "build/icon.ico"
    },
    "nsis": {
      "deleteAppDataOnUninstall": true,
      "oneClick": false,
      "perMachine": false,
      "allowToChangeInstallationDirectory": true,
      "license": "LICENSE.txt"
    },
    "win": {
      "icon": "build/icon.ico",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "ia32"
          ]
        }
      ]
    },
    "linux": {
      "target": "deb"
    }
  }
}
